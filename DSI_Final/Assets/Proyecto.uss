    margin-right: 6px;
    margin-left: 6px;
    -unity-background-image-tint-color: rgb(255, 255, 255);
}

.btnPanelActive {
    height: 93%;
    width: 10%;
    -unity-background-scale-mode: scale-to-fit;
    translate: 0 15.4%;
    background-color: rgb(146, 84, 192);
    border-left-color: rgb(81, 5, 143);
    border-right-color: rgb(81, 5, 143);
    border-top-color: rgb(81, 5, 143);
    border-bottom-color: rgb(81, 5, 143);
    border-top-width: 20px;
    border-right-width: 20px;
    border-bottom-width: 0;
