.btnPanel {
    flex-grow: 1;
    height: 93%;
    width: 10%;
    -unity-background-scale-mode: scale-to-fit;
    translate: 0 15.3%;
    background-color: rgb(146, 84, 192);
    border-left-color: rgb(81, 5, 143);
    border-right-color: rgb(81, 5, 143);
    border-top-color: rgb(81, 5, 143);
    border-bottom-color: rgb(81, 5, 143);
    border-top-width: 25px;
    border-right-width: 25px;
    border-bottom-width: 25px;
    border-left-width: 25px;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
    margin-right: 6px;
    margin-left: 6px;
    -unity-background-image-tint-color: rgb(142, 15, 190);
}

.btnPanel:hover {
    flex-grow: 1;
    height: 93%;
    width: 10%;
    -unity-background-scale-mode: scale-to-fit;
    translate: 0 15.3%;
    background-color: rgb(198, 158, 228);
    border-left-color: rgb(81, 5, 143);
    border-right-color: rgb(81, 5, 143);
    border-top-color: rgb(81, 5, 143);
    border-bottom-color: rgb(81, 5, 143);
    border-top-width: 25px;
